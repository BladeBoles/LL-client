{
  "version": 2,
  "alias": "lexical-library",
  "name": "lexical-library",
  "routes": [
    {
      "src": "^/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": ".*",
      "dest": "/index.html"
    },
    {
      "src": "^/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "^/favicon.ico", 
      "dest": "/favicon.ico"
    }
  ]
}